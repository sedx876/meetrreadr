<h2>Books</h2>

<ul id="books">
  <% @books.each do |book| %>
  
      <button><li><%= link_to book.title, book_path(book), class: 'book_title' %></li></button>

    
   
  <% end %>
</ul>

<button><%= link_to "Add a Book", new_book_path, class: "new_book" %></button>
<br><br>
<button><%= link_to "Book Tag List", tags_path %></button>


